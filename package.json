{
  "name": "pr-police",
  "version": "1.2.0",
  "description": "Slackbot that warns about open pull requests with given labels",
  "main": "index.js",
  "scripts": {
    "pretest": "standard",
    "test": "tape test/*.js | faucet",
    "start": "node index.js",
    "version": "standard-changelog"
  },
  "keywords": [
    "slackbot",
    "pull",
    "request",
    "github",
    "labels"
  ],
  "bin": {
    "pr-police": "./bin/pr-police"
  },
  "author": "rogeriopvl",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "pullhub": "^1.1.0",
    "slackbots": "^1.2.0",
    "ws": ">=3.3.1"
  },
  "devDependencies": {
    "commitlint": "^7.6.1",
    "faucet": "^0.0.1",
    "husky": "^1.3.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2",
    "standard": "^12.0.1",
    "standard-changelog": "^2.0.11",
    "tape": "^4.11.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e"
    }
  }
